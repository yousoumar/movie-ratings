{
  "expo": {
    "name": "Movie Ratings",
    "slug": "movie-ratings",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#40a857"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#40a857"
      }
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "description": "Movie ratings mobile application made with React Native & TypeScript.",
    "githubUrl": "https://github.com/yousoumar/movie-ratings"
  }
}
